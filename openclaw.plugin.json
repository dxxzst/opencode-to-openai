{
  "id": "opencode-proxy",
  "name": "OpenCode Proxy",
  "description": "OpenAI-compatible API gateway for the OpenCode CLI, enabling free Kimi/GLM/MiniMax models in OpenClaw and other clients.",
  "version": "1.1.0",
  "entry": "./src/plugin.js",
  "openclaw": {
    "extensions": ["./src/plugin.js"]
  },
  "configSchema": {
    "type": "object",
    "properties": {
      "enabled": { "type": "boolean", "default": true },
      "port": { "type": "integer", "default": 8083 },
      "apiKey": { "type": "string", "default": "" },
      "backendUrl": { "type": "string", "default": "http://127.0.0.1:4097" },
      "opencodePath": { "type": "string", "default": "opencode" }
    }
  }
}
